{
  "name": "@pinpatch/proxy",
  "version": "0.1.4",
  "description": "Reverse proxy with HTML overlay injection and WebSocket forwarding for Pinpatch.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alex1craig/pinpatch.git"
  },
  "keywords": [
    "pinpatch",
    "proxy",
    "overlay",
    "http-proxy",
    "websocket"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "echo 'lint not configured'",
    "clean": "rm -rf dist coverage"
  },
  "dependencies": {
    "@pinpatch/core": "workspace:*",
    "http-proxy": "^1.18.1"
  },
  "devDependencies": {
    "@types/http-proxy": "^1.17.15",
    "tsup": "^8.3.5",
    "vitest": "^3.0.6"
  }
}
