{
  "name": "@pinpatch/providers",
  "version": "0.1.3",
  "description": "Provider adapter registry and implementations for Pinpatch runtime execution.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alex1craig/pinpatch.git"
  },
  "keywords": [
    "pinpatch",
    "providers",
    "codex",
    "claude",
    "adapters"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "echo 'lint not configured'",
    "clean": "rm -rf dist coverage"
  },
  "dependencies": {
    "@pinpatch/core": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "vitest": "^3.0.6"
  }
}
